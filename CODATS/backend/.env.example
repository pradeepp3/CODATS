# CODATS Backend Environment Variables
# Copy this file to .env and add your API keys

# Port for the server
PORT=5000

# Gemini API Key (get from https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# OR Groq API Key (get from https://console.groq.com/keys)
GROQ_API_KEY=your_groq_api_key_here

# Choose which AI provider to use: "gemini" or "groq"
AI_PROVIDER=gemini
